{
  "name": "movie-crawler-react",
  "version": "0.0.1",
  "description": "koa+react",
  "main": "app.js",
  "scripts": {
    "start": "set NODE_ENV=product && node --harmony-async-await ./bin/www",
    "dev": "set DEBUG=debug && node-dev --harmony-async-await ./bin/www",
    "build:prod": "webpack --progress --watch --profile --colors --display-modules --display-error-details --display-cached --config webpack.prod.config.js",
    "build:dev": "webpack --progress --watch --profile --colors --display-modules --display-error-details --display-cached --config webpack.dev.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codelegant/movie-crawler-react.git"
  },
  "keywords": [
    "koa",
    "react",
    "webpack",
    "es6"
  ],
  "author": "laichuanfeng@hotmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codelegant/movie-crawler-react/issues"
  },
  "homepage": "https://github.com/codelegant/movie-crawler-react#readme",
  "dependencies": {
    "chalk": "^1.1.3",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-compress": "^2.0.0",
    "koa-csrf": "^3.0.4",
    "koa-favicon": "^2.0.0",
    "koa-logger": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0",
    "koa-views": "^5.0.2",
